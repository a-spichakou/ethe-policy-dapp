echo Compiling Sol files

solc --abi --bin --overwrite ./src/claimapi.sol --output-dir ./target/sol
solc --abi --bin --overwrite ./src/policy.sol --output-dir ./target/sol
solc --abi --bin --overwrite ./src/claimdispatch.sol --output-dir ./target/sol
solc --abi --bin --overwrite ./src/claimoraclelookup.sol --output-dir ./target/sol

ls ./target/sol -l

echo Copy install JS scripts

cp -r ./build/* ./target